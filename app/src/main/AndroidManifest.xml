<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="hsenglishsuggestionabenglsih11.example.class11wbenglish">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission
        android:name="android.permission.GET_TASKS"/>
    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission
        android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/koho"
        android:label="@string/app_name"
        android:roundIcon="@drawable/koho"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ModelPaper"></activity>
        <activity android:name=".QuizHomeActivity" />
        <activity android:name=".pdfviewActivity" />
        <activity android:name=".LAQShowActivity" />
        <activity android:name=".MCQShowActivity" />
        <activity android:name=".LAQChapterActivity" />
        <activity android:name=".MCQChapterActivity" />
        <activity android:name=".ContactUSActivity" />
        <activity android:name=".AboutusActivity" />
        <activity android:name=".Class11HomeActivity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".PastyearmainhomeActivity" />
        <activity android:name=".FlashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <service android:name=".services.MyFireBaseInstanceService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>